! Xresource file

! Terminal config (URxvt)

URxvt*.transparent: true
URxvt*.shading: 10

*background: #000000
*foreground: #00A1F2
! x;3y;4z x=style y=fg (colory) z=bg (colorz+8)
! Black + DarkGrey
!*color0: #5b5b5b
!*color8: #737373
! DarkRed + Red
!*color1: #9c748f
!*color9: #c392b3
! DarkGreen + Green
!*color2: #615375
!*color10: #7a6994
! DarkYellow + Yellow
*color3: #FFE46B
*color11: #F0C31F
! DarkBlue + Blue
*color4: #2F00C9
*color12: #002BD9
! DarkMangenta + Mangenta
*color5: #D900C0
*color13: #D156C3
! DarkCyan + Cyan
!*color6: #1f91cc
!*color14: #27b5ff
! LightGrey + White
!*color7: #90b3b3
!*color15: #b3dede
*utf8: 1
URxvt*scrollBar: false
URxvt*jumpScroll: true
URxvt*scrollTtyOutput: false
URxvt*scrollTtyKeypress: true
URxvt*mouseWheelScrollPage: false
!URxvt.font: 6x13

URxvt.font: xft:Inconsolata :size=14:antialias=true
URxvt*iso14755: False
URxvt.perl-ext-common: default,matcher,clipboard
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
